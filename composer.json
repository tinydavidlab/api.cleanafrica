{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "albertcht/lumen-helpers": "dev-dev-l9",
        "anik/form-request": "^4.5",
        "benwilkins/laravel-fcm-notification": "dev-master",
        "doctrine/dbal": "^3.5",
        "dusterio/lumen-passport": "^0.3",
        "flipbox/lumen-generator": "^9.1",
        "laravel/lumen-framework": "^v9.0",
        "laravel/tinker": "^v2.7",
        "league/csv": "^9.8",
        "league/flysystem-aws-s3-v3": "^1.0",
        "prettus/l5-repository": "^2.8",
        "spatie/laravel-fractal": "^6.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.0",
        "fakerphp/faker": "^v1.20",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tinydavidlab/lumen-helpers"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tinydavidlab/laravel-fcm-notification"
        }
    ]
}
